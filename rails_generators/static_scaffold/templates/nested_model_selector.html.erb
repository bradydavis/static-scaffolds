<%if gen_spec.nests_many%>
<div id="model_selector">
  <ul>
    <%for nested_resource in gen_spec.nests_many.map{|x| GenSpecFactory.constantize(x[:model])}%>
      <li> <%%=link_to <%=nested_resource.plural_title.inspect%>, <%=nested_resource.collection_route%>, :class=>"ui-corner-tl ui-corner-tr"%> </li>
    <%end%>
  </ul>
</div>
<%else%>
  <%%=render :partial=>"/shared/root_model_selector"%>
<%end%>
